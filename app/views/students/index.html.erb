<h1>All Students</h1>

<% @students.each do |student| %>
  <ul class="student-<%= student.id %>">
    <%= link_to student.name, student_path(student) %>
    <%= link_to "Delete", student_path(student), method: :delete %>
  </ul>
<% end %>

<%= link_to "Create a New Student!", new_student_path %>
<%= link_to "See All Students!", students_path %>
